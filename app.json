{
  "expo": {
    "name": "AquaCredit",
    "slug": "AquaCredit",
    "version": "1.0.0",
    "owner": "pentapillisuresh",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aquacredit",
    "userInterfaceStyle": "automatic",
    "useNextNotificationsApi": true,
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sureshpentapilli.aquacredit",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-build-properties",
      "@react-native-firebase/app"
    ],
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "08248e48-60f1-456a-b9f0-32f8a75b08c3"
      }
    },
    "android": {
      "package": "com.sureshpentapilli.aquacredit",
      "googleServicesFile": "./android/app/google-services.json",
      "useNextNotificationsApi": true
    }
  }
}
